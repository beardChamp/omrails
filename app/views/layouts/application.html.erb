<!DOCTYPE html>
<html lang="en">
    <head>
        <title>One Month Rails</title>
        <%= stylesheet_link_tag    "application", :media => "all" %>
        <%= javascript_include_tag "application" %>
        <%= csrf_meta_tags %>
    </head>
    <body>
        <%= render 'layouts/header' %>

        <div class="container">
            <div id="content">
                <%= yield %>
            </div>

            <%= render 'layouts/footer' %>

        </div>

    </body>
</html>
